{
    "name": "tagtwp/twpstrap",
    "type": "wordpress-theme",
    "description": "Hi. I'm a starter theme called twpstrap, or underscores, if you like. I'm a theme meant for hacking so don't use me as a Parent Theme. Instead try turning me into the next, most awesome, WordPress theme out there. That's what I'm here for.",
    "keywords": [
        "WordPress",
        "Themes"
    ],
    "homepage": "https://github.com/tagtwp/twpstrap",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Contributors",
            "homepage": "https://github.com/tagtwp/twpstrap/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^v1.0.0",
        "wptrt/wpthemereview": "^0.2.1",
        "php-parallel-lint/php-parallel-lint": "^v1.3.2",
        "wp-cli/i18n-command": "^v2.4.3"
    },
    "scripts": {
        "lint:wpcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
        "lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor .",
        "make-pot": "wp i18n make-pot . languages/twpstrap.pot"
    },
    "support": {
        "issues": "https://github.com/tagtwp/twpstrap/issues",
        "source": "https://github.com/tagtwp/twpstrap"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
